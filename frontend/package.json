{
  "name": "ng-blog",
  "version": "1.0.0",
  "description": "Blog about Java, Angular 2, MongoDb, Docker, Unreal Engine",
  "scripts": {
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "clean": "npm cache clean && npm run rimraf -- node_modules dist",
    "clean:install": "npm set progress=false && npm install",
    "clean:start": "npm start",
    "preclean:install": "npm run clean",
    "preclean:start": "npm run clean",
    "start": "webpack-dev-server --colors --watch --port 3001 --content-base src/public --inline --hot"
  },
  "dependencies": {
    "@angular/common": "~2.4.10",
    "@angular/compiler": "~2.4.10",
    "@angular/core": "~2.4.10",
    "@angular/forms": "~2.4.10",
    "@angular/http": "~2.4.10",
    "@angular/platform-browser": "~2.4.10",
    "@angular/platform-browser-dynamic": "~2.4.10",
    "@angular/router": "~3.4.10",

    "core-js": "^2.4.1",
    "rxjs": "^5.2.0",
    "zone.js": "^0.8.4",

    "@ngrx/core": "~1.2.0",
    "@ngrx/store": "~2.2.1",
    "@ngrx/effects": "~2.0.1",
    "@ngrx/store-devtools": "~3.2.2",
    "@ngrx/store-log-monitor": "~3.0.2",
    "ngrx-store-logger": "~0.1.7"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.37",
    "@types/node": "^7.0.8",
    "typescript": "^2.1.15",
    "node-sass": "^4.5.0",
    "rimraf": "^2.6.1",

    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.1.2",
    "css-loader": "^0.27.3",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.5",
    "to-string-loader": "^1.1.5",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",

    "webpack": "~2.2.1",
    "webpack-dev-server": "~2.4.2",
    "webpack-merge": "~4.1.0",
    "html-webpack-plugin": "~2.28.0",
    "extract-text-webpack-plugin": "~2.1.0"
  },
  "author": "Alexey (hiper2d) Zelenovsky",
  "license": "ISC"
}
